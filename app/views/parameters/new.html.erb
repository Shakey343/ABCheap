<div class="newplanet">
  <div class="title" style="text-align:center">
    <div style="width: 100%; height: 200px;"
  data-controller="mapbox"
  data-mapbox-markers-value="<%= @markers.to_json %>"
  data-mapbox-api-key-value="<%= ENV['MAPBOX_API_KEY'] %>"></div>
  </div>
  <div class="rounded">
      <%= simple_form_for @parameter do |f| %>
      <%= f.input :origin %>
      <%= f.input :destination %>
      <%= f.input :preferred_start, as: :date, html5: true %>
      <%= f.input :earliest_start %>
      <%= f.input :latest_finish %>
      <%= f.submit class: 'btn btn-block', value: 'New Parameter' %>
      <% end %>
  </div>
</div>
